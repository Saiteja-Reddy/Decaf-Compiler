class Program
{
// max size of array 100
	int arr[100];
	int i;
	int n;
	int search;
	int flag;

	void main()
	{
		flag = 0;

		callout("printf", "Enter the number of elements in the array: ");
		
		n = scanInt();

		callout("printf", "Enter the elements in the array: \n");

		for i = 1, n
		{
			arr[i] = scanInt();
		}

		callout("printf", "Enter the search element in the array: ");
		
		search = scanInt();

		for i = 1, n
		{
			if (arr[i] == search)
			{
				flag = 1;
				callout("printf", "Found the element in the array\n");
				break;
			}
		}

		if (flag == 0)
		{
			callout("printf", "Did not find the element in the array\n");
		}
		return;

	}

}